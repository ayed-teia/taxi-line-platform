# Pilot Mode Configuration
# Copy to .env for pilot deployment

VITE_APP_MODE=pilot
VITE_FIREBASE_API_KEY=your-real-api-key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-real-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123:web:abc

# Emulators are automatically blocked in pilot mode
VITE_USE_EMULATORS=false
VITE_EMULATOR_HOST=127.0.0.1
